# OpenAI API key for accessing GPT models
OPENAI_API_KEY=your_openai_api_key

# Secret key for Chainlit authentication
CHAINLIT_AUTH_SECRET=your_chainlit_auth_secret

# Literal API key for monitoring OpenAI usage
LITERAL_API_KEY=your_literal_api_key

# OAuth configuration for GitHub
OAUTH_GITHUB_CLIENT_ID=your_github_client_id
OAUTH_GITHUB_CLIENT_SECRET=your_github_client_secret

# OAuth configuration for GitLab
OAUTH_GITLAB_CLIENT_ID=your_gitlab_client_id
OAUTH_GITLAB_CLIENT_SECRET=your_gitlab_client_secret

# Domain for GitLab OAuth (default: gitlab.com)
OAUTH_GITLAB_DOMAIN="gitlab.com"